  },
  "homepage": "https://github.com/Shopify/quilt/blob/master/packages/react-graphql/README.md",
  "dependencies": {
    "@apollo/react-common": ">=3.0.0 <4.0.0",
    "@apollo/react-hooks": ">=3.0.0 <4.0.0",
    "@shopify/async": "1.0.0-sknext-beta.1",
    "@shopify/react-async": "1.0.0-sknext-beta.1",
    "@shopify/react-effect": "1.0.0-sknext-beta.1",
    "@shopify/react-hooks": "1.0.0-sknext-beta.1",
